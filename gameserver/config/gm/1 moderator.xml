<?xml version='1.0' encoding='utf-8'?>
<list>
	<!-- Используется для настройки EVERYBODY_HAS_ADMIN_RIGHTS -->
	<char>
		<PlayerID set="-1"/>
		
		<!-- Маркер на то, что персонаж является ГМским (убраны некоторые проверки на персонажа и т.д.) -->
		<IsGM set="false"/>
		
		<!-- Без этих прав невозможно использовать вообще никакие админ команды -->
		<CanUseGMCommand set="false"/>
		<!--
			1 - Moderator
            2 - Sheriff
            3 - Support
            4 - Event Game Master
            5 - Head Game Master
            6 - Administrator
		-->
		<AccessType set="1"/>

		<!-- Права на использование ГМ чата (общение между ГМами) -->
		<CanUseGMChat set="false"/>
		
		<!-- Право банить чат -->
		<CanBanChat set="false"/>
		<!-- Максимально разрешенный срок (в минутах), -1 отключено -->
		<BanChatMaxValue set="900"/>
		
		<!-- По умолчанию TRUE для всех игроков -->
		<!-- Права на использование трейда/почты -->
		<UseTrade set="true"/>
		<!-- Права на использование автоатаки -->
		<CanAttack set="true"/>
		<!-- Права на использование инвентаря (открыть/закрыть) -->
		<UseInventory set="true"/>
		<!-- Права на рекомендации -->
		<CanEvaluate set="true"/>
		<!-- Права на присоединение к пати -->
		<CanJoinParty set="true"/>
		<!-- Права на вступление в клан -->
		<CanJoinClan set="false"/>
		<!-- Права на использование ВХ -->
		<UseWarehouse set="true"/>
		<!-- Права на использование НПЦ магазинов -->
		<UseShop set="true"/>
		<!-- Права на телепортация (не рекол!) -->
		<UseTeleport set="true"/>
	</char>
</list>